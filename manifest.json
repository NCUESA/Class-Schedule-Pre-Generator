{
  "name": "預選課程小插件",
  "version": "1.3",
  "manifest_version": 3,
  "action": {
    "default_title": "點擊啟動"
  },
  "icons": {
    "64": "./select64.png"
  },
  "description": "用以在彰師查看課程清單時，可以自己先預選課。",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "jquery-3.6.4.min.js"
      ],
      "matches": []
    }
  ]
}